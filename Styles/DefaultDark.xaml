<!--  ModernControls Default Dark Theme  -->
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--  Background  -->


    <SolidColorBrush x:Key="BackgroundDark"
                     Color="#FF2E343E" />

    <SolidColorBrush x:Key="BackgroundLight"
                     Color="#4E5A6E" />

    <SolidColorBrush x:Key="BackgroundMain"
                     Color="#3B4453" />
    

    <!--  Foreground  -->
   
    
    
    
    <SolidColorBrush x:Key="TextNormal"
                     Color="#CDD0D6" />
    
    
    
    


    <!--Border-->
    <SolidColorBrush x:Key="BorderDark"
                     Color="#FF2C333A" />
    <SolidColorBrush x:Key="BorderLight"
                     Color="#FF5D6269" />


    <!--Disable-->
    <SolidColorBrush x:Key="DisableDark"
                     Color="#FF303438" />
    <SolidColorBrush x:Key="DisableLight"
                     Color="#ACB2BB" />

