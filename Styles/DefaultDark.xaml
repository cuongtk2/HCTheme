    <SolidColorBrush x:Key="BlueDark"
                     Color="#0152c6" />

    <SolidColorBrush x:Key="CyalDark"
                     Color="#FF023A48" />




    <!--  Status  -->
    <SolidColorBrush x:Key="success"
